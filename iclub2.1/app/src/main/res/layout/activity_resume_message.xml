<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/iculd_beijin_icon"

    tools:context="com.example.administrator.iclub21.util.ResumeMessageActivity"
   >

    <ProgressBar
        android:id="@+id/progressbar"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:visibility="gone"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
         />
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <include layout="@layout/resume_message_tailt_layout"/>
    <LinearLayout
          android:visibility="gone"
        android:id="@+id/epenrience_particular"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <EditText
            android:gravity="top"
            android:textSize="@dimen/textSize15"
            android:background="#3Affffff"
            android:textColor="@color/white"
            android:id="@+id/resume_info_et"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="10dp"
            android:layout_width="match_parent"
            android:layout_height="200dp" />

        <TextView
            android:text="提示:"
            android:layout_marginLeft="25dp"
            android:textColor="@color/white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/onself_base"

            android:layout_marginLeft="25dp"
            android:textColor="@color/white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/oneself_experience"
            android:layout_marginLeft="25dp"
            android:textColor="@color/white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    </LinearLayout>
    <LinearLayout
        android:id="@+id/work_experience_particular"
        android:visibility="gone"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <EditText
            android:textColor="@color/white"
            android:id="@+id/resumeWorkExperience_et"
            android:gravity="top"
            android:background="#3Affffff"
            android:textSize="@dimen/textSize15"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="10dp"
            android:layout_width="match_parent"
            android:layout_height="200dp" />
        <TextView
            android:text="提示:"
            android:layout_marginLeft="25dp"
            android:textColor="@color/white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/base_work"
            android:layout_marginLeft="25dp"
            android:textColor="@color/white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/work_performance"
            android:layout_marginLeft="25dp"
            android:textColor="@color/white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/add_image_layout"
        android:visibility="gone"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_marginLeft="30dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="提示："
                android:layout_marginTop="10dp"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_marginTop="10dp"
                android:textColor="@color/white"
                android:text="可在电脑打开以下网址链接，上传跟个人工作
相关的图片文件(用户名和密码跟app登录一致)"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>
        <ImageView
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:background="@mipmap/weil_one_icon"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
        <TextView
            android:text="www.iclubapps.com"
            android:gravity="center"
            android:textColor="@color/whiteOne"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />


        <LinearLayout
            android:layout_marginTop="10dp"
            android:orientation="vertical"
            android:id="@+id/show_add_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:id="@+id/add_uplaod_image_layout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_marginLeft="10dp"
                    android:background="#20000000"
                    android:layout_width="90dp"
                    android:layout_height="90dp" />
                <TextView
                    android:layout_marginLeft="40dp"
                    android:background="@mipmap/add_image_icon"
                    android:layout_width="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:layout_height="wrap_content" />
            </RelativeLayout>
            <com.example.administrator.iclub21.view.WordWrapView
                android:id="@+id/view_wordwrap"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                />
        </LinearLayout>


    </LinearLayout>
    <ScrollView
        android:layout_marginBottom="30dp"
        android:scrollbars="none"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    <LinearLayout
        android:id="@+id/add_video_layout"
        android:visibility="gone"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_marginLeft="30dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="提示："
                android:layout_marginTop="10dp"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:textColor="@color/white"
                android:layout_marginTop="10dp"
                android:text="本地视频上传速度较慢，建议拍摄上传，也可
以在网站上上传，可在电脑打开以下网址链接
,上传跟个人工作相关的视频文件(用户名
和密码跟app登录的一样)"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>
        <ImageView
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:background="@mipmap/weil_one_icon"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
        <TextView
            android:text="www.iclubapps.com"
            android:gravity="center"
            android:textColor="@color/whiteOne"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
        <RelativeLayout
            android:id="@+id/upload_video_layout"
            android:layout_marginRight="30dp"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="20dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:background="#20000000"
                android:layout_width="match_parent"
                android:layout_height="160dp" />
            <TextView

                android:layout_marginLeft="10dp"
                android:background="@mipmap/add_image_icon"
                android:layout_width="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_height="wrap_content" />

        </RelativeLayout>

        <RelativeLayout
             android:visibility="gone"
            android:id="@+id/video_xshi_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <VideoView
                android:visibility="gone"
                android:id="@+id/video_view"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_width="match_parent"
                android:layout_height="160dp" />
            <ImageView
                android:src="@mipmap/ic_launcher"
                android:id="@+id/thumbnail_Iv"
                android:scaleType="fitXY"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_width="match_parent"
                android:layout_height="160dp" />
            <TextView
                android:id="@+id/action_video_bt"
                android:background="@mipmap/video_button_icon"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true" />
        </RelativeLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.example.administrator.iclub21.view.CustomHomeScrollListView
                android:id="@+id/video_view_wordwrap"
                android:layout_marginTop="10dp"
                android:dividerHeight="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                />

        </LinearLayout>
    </LinearLayout>
    </ScrollView>
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
    <LinearLayout
        android:id="@+id/add_music_layout"
        android:visibility="gone"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_marginLeft="30dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="提示："
                android:layout_marginTop="10dp"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_marginTop="10dp"
                android:textColor="@color/white"
                android:text="可在电脑打开以下网址链接，上传跟个人工作
 相关的音频文件(用户名和密码跟app登录一致)"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>
        <ImageView
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:background="@mipmap/weil_one_icon"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
        <TextView
            android:text="www.iclubapps.com"
            android:gravity="center"
            android:textColor="@color/white"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <RelativeLayout

            android:layout_marginTop="20dp"
            android:id="@+id/add_uplaod_music_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:background="@drawable/rounded_textview"
                android:layout_width="match_parent"
                android:layout_height="90dp"
              />
            <TextView

                android:layout_marginLeft="170dp"
                android:background="@mipmap/add_image_icon"
                android:layout_width="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_height="wrap_content" />

        </RelativeLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.example.administrator.iclub21.view.MusicWordWrapView
                android:id="@+id/music_view_wordwrap"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                />
            <com.example.administrator.iclub21.view.CustomHomeScrollListView
                android:id="@+id/music_list_view"
                android:layout_marginTop="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                />
        </LinearLayout>


    </LinearLayout>
        </ScrollView>

    </LinearLayout>
</RelativeLayout>
