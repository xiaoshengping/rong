<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@mipmap/iculd_beijin_icon"
    tools:context="com.example.administrator.iclub21.util.ResumeListParticularActivity">
     <include layout="@layout/resume_listparticualr_tailt_layout"/>

      <ScrollView
          android:scrollbars="none"
          android:layout_width="match_parent"
          android:layout_height="wrap_content">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
    <RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/resumeUserbg_iv"
        android:scaleType="fitXY"
        android:background="@mipmap/resume_background_icon"
        android:layout_width="match_parent"
        android:layout_height="250dp" />
    <LinearLayout
        android:orientation="vertical"
        android:layout_below="@+id/resumeUserbg_iv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout">
         <LinearLayout
             android:gravity="center"
             android:orientation="horizontal"
             android:layout_width="match_parent"
             android:layout_height="wrap_content">

             <TextView
                 android:id="@+id/resumeZhName_tv"
                android:layout_marginLeft="100dp"
                android:textSize="@dimen/textSize18"
                android:textColor="@color/white"
                android:text="昵称"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
             <ImageView
                 android:id="@+id/resumeSex_iv"
                 android:scaleType="fitXY"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content" />
             <TextView
                 android:id="@+id/resume_ViewCount_tv"
                 android:text="浏览量"
                 android:textSize="@dimen/textSize12"
                 android:textColor="@color/white"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content" />
         </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:gravity="center"
                android:layout_weight="1"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="年龄"
                    android:textSize="@dimen/textSize12"
                    android:textColor="@color/white"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/resumeAge_tv"
                    android:text="年龄"
                    android:textSize="@dimen/textSize12"
                    android:textColor="@color/white"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

            </LinearLayout>

            <LinearLayout
                android:gravity="center"
                android:layout_weight="1"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="工作地点"
                    android:textSize="@dimen/textSize12"
                    android:textColor="@color/white"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/resume_WorkPlace_tv"
                    android:text="工作地点"
                    android:textSize="@dimen/textSize12"
                    android:textColor="@color/white"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

            </LinearLayout>
            <LinearLayout
                android:gravity="center"
                android:layout_weight="1"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="职位"
                    android:textSize="@dimen/textSize12"
                    android:textColor="@color/white"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:text="职位"
                    android:id="@+id/resumeJobName_isd_tv"
                    android:textSize="@dimen/textSize12"
                    android:textColor="@color/white"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />


            </LinearLayout>


        </LinearLayout>


    </LinearLayout>

        <ImageView
            android:id="@+id/usericon_background_iv"
            android:src="@mipmap/ic_launcher"
            android:scaleType="fitXY"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_above="@+id/linearLayout"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="53dp"
            android:layout_marginStart="53dp" />
    </RelativeLayout>

        </RelativeLayout>
<LinearLayout
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:text="自我介绍"
        android:gravity="center"
        android:drawablePadding="20dp"
        android:layout_marginLeft="30dp"
         android:drawableLeft="@mipmap/oneself_info_icon"
        android:textSize="@dimen/textSize12"
        android:textColor="@color/white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/resumeInfo_tv"
        android:text="内容lsdksakdjfjfjfjjdjdj"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="5dp"
        android:textSize="@dimen/textSize12"
        android:textColor="@color/white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />


</LinearLayout>
    <LinearLayout
        android:layout_marginTop="10dp"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:text="工作经历"
            android:gravity="center"
            android:drawablePadding="20dp"
            android:layout_marginLeft="30dp"
            android:drawableLeft="@mipmap/work_expelt_icon"
            android:textSize="@dimen/textSize12"
            android:textColor="@color/white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/resume_WorkExperience_tv"
            android:text="内容lsdksakdjfjfjfjjdjdj"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="5dp"
            android:textSize="@dimen/textSize12"
            android:textColor="@color/white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />


    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="10dp"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:text="联系方式"
            android:gravity="center"
            android:drawablePadding="20dp"
            android:layout_marginLeft="30dp"
            android:drawableLeft="@mipmap/relation_icon"
            android:textSize="@dimen/textSize12"
            android:textColor="@color/white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <LinearLayout
            android:layout_marginTop="5dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="QQ"
                android:layout_marginLeft="30dp"
                android:textSize="@dimen/textSize12"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/resumeQq_tv"
                android:text="QQ"
                android:layout_marginLeft="45dp"
                android:textSize="@dimen/textSize12"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="5dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="Email"
                android:layout_marginLeft="30dp"
                android:textSize="@dimen/textSize12"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/resumeEmail_tv"
                android:text="Email"
                android:layout_marginLeft="30dp"
                android:textSize="@dimen/textSize12"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="5dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="手机号码"
                android:layout_marginLeft="30dp"
                android:textSize="@dimen/textSize12"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/resumeMobile_tv"
                android:text="手机号码"
                android:layout_marginLeft="10dp"
                android:textSize="@dimen/textSize12"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="10dp"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        <TextView
            android:text="图片介绍"
            android:gravity="center"
            android:drawablePadding="20dp"
            android:layout_marginLeft="30dp"
            android:drawableLeft="@mipmap/picture_recommend_icon"
            android:textSize="@dimen/textSize12"
            android:textColor="@color/white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
            <TextView
                android:text="更多"
               android:layout_marginLeft="200dp"
                android:textSize="@dimen/textSize12"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            </LinearLayout>
        <LinearLayout
            android:id="@+id/listparticual_picture_layout"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/show_picture_tv"
                android:text="无相关照片"
                android:gravity="center"
                android:visibility="gone"
                android:layout_marginTop="5dp"
                android:textSize="@dimen/textSize12"
                android:textColor="@color/white"
                android:layout_width="match_parent"
                android:layout_height="80dp" />
            <com.example.administrator.iclub21.view.WordWrapView
                android:id="@+id/view_wordwrap"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </LinearLayout>



    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="10dp"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout

            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        <TextView
            android:text="视频展示"
            android:gravity="center"
            android:drawablePadding="20dp"
            android:layout_marginLeft="30dp"
            android:drawableLeft="@mipmap/video_icon"
            android:textSize="@dimen/textSize12"
            android:textColor="@color/white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
             <ToggleButton
                 android:drawableRight="@drawable/toggle_button_selector"
                 android:background="@null"
                 android:textOff="@null"
                 android:textOn="@null"
                 android:layout_marginRight="30dp"
                 android:id="@+id/video_togglButton"
                 android:layout_width="match_parent"
                 android:layout_height="wrap_content" />
        </LinearLayout>
        <RelativeLayout
            android:id="@+id/listparticual_video_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        <TextView
            android:id="@+id/on_video_textview"
            android:visibility="gone"
            android:text="无相关视频"
            android:gravity="center"
            android:layout_marginTop="5dp"
            android:textSize="@dimen/textSize12"
            android:textColor="@color/white"
            android:layout_width="match_parent"
            android:layout_height="100dp" />
            <ImageView
                android:visibility="gone"
                android:scaleType="fitXY"
                android:id="@+id/show_video_imageView"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_width="match_parent"
                android:layout_height="150dp" />
            <TextView
                android:visibility="gone"
                android:id="@+id/show_video_button_tv"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:background="@mipmap/video_button_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </RelativeLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="10dp"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        <TextView
            android:text="音乐作品"
            android:gravity="center"
            android:drawablePadding="20dp"
            android:layout_marginLeft="30dp"
            android:drawableLeft="@mipmap/music_icon"
            android:textSize="@dimen/textSize12"
            android:textColor="@color/white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
            <ToggleButton
                android:drawableRight="@drawable/toggle_button_selector"
                android:background="@null"
                android:textOff="@null"
                android:textOn="@null"
                android:checked="true"
                android:layout_marginRight="30dp"
                android:id="@+id/music_togglButton"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/listparticual_music_layout"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="150dp">
        <TextView
            android:visibility="gone"
            android:id="@+id/no_music_tv"
            android:text="无相关音乐"
             android:gravity="center"
            android:layout_marginTop="5dp"
            android:textSize="@dimen/textSize12"
            android:textColor="@color/white"
            android:layout_width="match_parent"
            android:layout_height="50dp" />
            <com.example.administrator.iclub21.view.WordWrapView
                android:id="@+id/show_music_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
         </LinearLayout>

    </LinearLayout>
    </LinearLayout>
      </ScrollView>
</LinearLayout>
